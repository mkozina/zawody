<script src="js/ref.js"></script>

<div class="container">

<h1>Panel sędziego</h1>

<p>Weź udział w zawodach:

<div class="row">
<div class="form-group col-xs-4">
<select id="contest" class="form-control">
<% contests.forEach(function(contest) { %>
<option value="<%= contest %>"><%= contest %></option>
<% }); %>
</select>
</div>
</div>

<form class="form" action="/contestref" method="get">
	<input id="contestname" type="hidden" name="name" value="" />
	<div class="row">
	<div class="form-group col-xs-4">
	<button type="submit" class="btn btn-primary">Rozpocznij</button>
	</div>
	</div>
</form>

</p>

<p><a href="/logout">Wyloguj się</a></p>

</div>
